{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "react-app",
        "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "off",
        "react/prop-types": "off",
        "react-hooks/exhaustive-deps": "off"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ]
}